{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:vue/essential",
        "standard"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "vue"
    ],
    "rules": {
        "comma-dangle": "off",
        "vue/no-multiple-template-root": "off",
        "quotes": [
            "error",
            "double"
        ],
        "no-labels": "off",
        "no-unused-vars": "off",
        "space-before-function-paren": [
            "error",
            "never"
        ],
        "no-undef": "off"
    }
}